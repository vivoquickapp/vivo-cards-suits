<template>
  <div class="wrap">
    <image class="image" src="{{imgUrl}}"></image>
    <text class="content">{{ content }}</text>
    <text class="btn" show="{{!!button}}" onclick="handleClickBtn">{{
      button
    }}</text>
  </div>
</template>

<script>
export default {
  props: {
    status: {
      default: "blank" // blank network
    },
    content: {
      default: ""
    },
    button: {
      default: ""
    }
  },
  data() {
    return {};
  },
  computed: {
    imgUrl() {
      if (this.status == "network") {
        return "data:image/svg+xml;base64,";
      } else {
        return "data:image/svg+xml;base64,";
      }
    }
  },
  handleClickBtn() {
    this.$emti("clickbtn");
  }
};
</script>

